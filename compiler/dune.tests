(rule
  (deps (source_tree cyclic_test.mls))
  (target cyclic_test.output)
  (action
    (run ../main.exe -test-mode cyclic_test.mls)
  )
)
(rule
  (alias runtest)
  (deps (source_tree cyclic_test.expected) cyclic_test.output)
  (action (diff cyclic_test.expected cyclic_test.output))
)

